<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1"
      fx:controller="ModifyFileController">
    <children>
        <MenuBar VBox.vgrow="NEVER">
            <menus>
                <Menu mnemonicParsing="false" text="File">
                    <items>
                        <MenuItem fx:id="menuNew" mnemonicParsing="false" onAction="#openNewFile" text="New"/>
                        <MenuItem fx:id="menuOpen" mnemonicParsing="false" onAction="#openFile" text="Openâ€¦"/>
                        <MenuItem fx:id="menuSave" mnemonicParsing="false" onAction="#saveFile" text="Save"/>
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <TextArea fx:id="textArea" prefHeight="372.0" prefWidth="640.0" wrapText="true">
            <padding>
                <Insets bottom="2.0" left="2.0" right="2.0" top="2.0"/>
            </padding>
            <VBox.margin>
                <Insets/>
            </VBox.margin>
        </TextArea>
    </children>
</VBox>
